package com.vtech.iocdemo;

public class Car {

	private Engine engine;

	//Constructor Injection
	public Car(Engine engine) {
		super();
		this.engine = engine;
	}

	public Engine getEngine() {
		return engine;
	}

	//Setter Injection
	public void setEngine(Engine engine) {
		this.engine = engine;
	}
}
