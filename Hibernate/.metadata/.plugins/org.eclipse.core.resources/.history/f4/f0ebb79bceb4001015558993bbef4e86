package com.vtech.ams.model;

import java.util.Date;

public class Attendance {

	private int id;
	private String name;
	private long mobile;
	private boolean attended;
	
	public Attendance(String name, long mobile, boolean attended) {
		super();
		this.name = name;
		this.mobile = mobile;
		this.attended = attended;
	}
	
	

}
